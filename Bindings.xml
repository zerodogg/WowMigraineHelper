<Bindings>
  <Binding name="TOGGLEMIGRAINEBARS" runOnUp="false" header="MIGRAINEHELPER" default="ALT-CTRL-M">
      MigraineHelper:ToggleEdgeOverlay();
 </Binding>
  <Binding name="TOGGLEMIGRAINEOVERLAY" runOnUp="false" header="MIGRAINEHELPER" default="ALT-CTRL-D">
      MigraineHelper:ToggleOpacityOverlay();
 </Binding>
</Bindings>
